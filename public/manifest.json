{
  "name": "plan.pixelplus.ru Injection",
  "manifest_version": 2,
  "version": "1.6.9",
  "permissions": [
    "tabs",
    "https://plan.pixelplus.ru/*",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "plan-injection.js"
      ],
      "matches": [
        "https://plan.pixelplus.ru/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "50": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html"
}